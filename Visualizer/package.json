{
  "private": true,
  "name": "@hpcc-js/visualizer",
  "version": "2.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf lib* types dist res/*.js res/*.map",
    "compile-es6": "tsc --module es6 --outDir ./lib-es6",
    "compile-es6-watch": "npm run compile-es6 -- -w",
    "compile-umd": "tsc --module umd --outDir ./lib-umd",
    "bundle": "rollup -c",
    "bundle-watch": "npm run bundle -- -w",
    "minimize": "uglifyjs dist/index.js -c -m -o res/index.min.js",
    "build": "run-s compile-es6 bundle minimize",
    "watch": "run-p compile-es6-watch bundle-watch",
    "link-hpcc-js": "mklink /J hpcc-js ..\\hpcc-js",
    "lint": "tslint --project . src/**/*.ts",
    "docs": "typedoc --options tdoptions.json ."
  },
  "devDependencies": {
    "d3-fetch": "1.1.2",
    "@hpcc-js/api": "2.0.3",
    "@hpcc-js/chart": "2.1.2",
    "@hpcc-js/codemirror": "2.0.4",
    "@hpcc-js/codemirror-shim": "2.0.2",
    "@hpcc-js/common": "2.2.1",
    "@hpcc-js/comms": "2.0.3",
    "@hpcc-js/composite": "2.0.4",
    "@hpcc-js/dgrid": "2.0.4",
    "@hpcc-js/dgrid-shim": "2.0.3",
    "@hpcc-js/form": "2.0.3",
    "@hpcc-js/graph": "2.2.0",
    "@hpcc-js/layout": "2.1.3",
    "@hpcc-js/map": "2.0.4",
    "@hpcc-js/other": "2.1.3",
    "@hpcc-js/phosphor": "2.1.3",
    "@hpcc-js/phosphor-shim": "2.0.2",
    "@hpcc-js/timeline": "2.0.3",
    "@hpcc-js/tree": "2.0.3",
    "@hpcc-js/util": "2.1.1",
    "npm-run-all": "4.1.3",
    "rimraf": "2.6.2",
    "rollup": "0.63.5",
    "rollup-plugin-alias": "1.4.0",
    "rollup-plugin-commonjs": "9.1.4",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-postcss": "1.6.2",
    "tslib": "1.9.3",
    "tslint": "5.11.0",
    "typescript": "3.0.3",
    "uglify-js": "3.4.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GordonSmith/Visualizer.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hpcc-systems/Visualizer/issues"
  },
  "homepage": "https://github.com/hpcc-systems/Visualizer#readme"
}
